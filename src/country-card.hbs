<div class="country-info__card">
  <div class="country-info__img-top">
    <img src="{{flags.svg}}" alt="{{name.official}}" width="50">
  </div>
  <div class="country-info__thumb">
    <h2 class="country-info__name"> {{name.official}}</h2>
    <p class="country-info__capital">Capital: {{capital}}</p>
    <p class="country-info__population">Population: {{population}}</p>
    <p class="country-info__languages">Languages:</p>
    <ul>
                {{#each languages}}
                <li class="country-info__lang">{{[]}}</li>
                {{/each}}
            </ul>

    </ul>
  </div>
</div>


 {{!-- 
Official Name : name.official
Capital : ,capital
Population : ,population
Flag : ,flag
Languages : ,languages 
  --}}