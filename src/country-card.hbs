<div class="country">
  <div class="country-img-top">
    <img src="{{flags.svg}}" alt="{{name.official}}" width="50">
  </div>
  <div class="country-body">
    <h2 class="country-name"> {{name.official}}</h2>
    <p class="country-capital">Capital: {{capital}}</p>
    <p class="country-population">Population: {{population}}</p>
    <p class="country-languages">Languages:</p>
    <ul>
                {{#each languages}}
                <li class="lang">{{[]}}</li>
                {{/each}}
            </ul>

    </ul>
  </div>
</div>


 {{!-- 
Official Name : name.official
Capital : ,capital
Population : ,population
Flag : ,flag
Languages : ,languages 
  --}}